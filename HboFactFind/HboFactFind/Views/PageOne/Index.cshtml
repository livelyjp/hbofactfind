@model IEnumerable<HboFactFind.Domain.Pages.PageOne>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Id)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ClientOneTitle)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ClientOneForename)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ClientOneSurnames)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CreatedDateTime)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ClientOneTitle)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ClientOneForename)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ClientOneSurnames)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CreatedDateTime)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new {id = item.Id}) |
                @Html.ActionLink("Details", "Details", new {id = item.Id}) |
                @Html.ActionLink("Delete", "Delete", new {id = item.Id})
            </td>
        </tr>
    }

</table>