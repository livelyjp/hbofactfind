@model IEnumerable<HboFactFind.Domain.Questions.Dependants.FinancialDependant>

<table class="table financialDependantsTable">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.DependantName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DateOfBirth)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DependantOn)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.AgeOfIndependance)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.RelationshipAndReason)
        </th>
    </tr>
@if (Model.Count() == 0)
{
    <tr>
        <td colspan="10" class="empty">
            No Dependants
        </td>
    </tr>
}
else
{
    foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.DependantName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DateOfBirth)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DependantOn)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.AgeOfIndependance)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.RelationshipAndReason)
            </td>
        </tr>
    }
}
    <tr>
</table>
